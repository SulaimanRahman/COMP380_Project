<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<BorderPane prefHeight="200.0" prefWidth="400.0" stylesheets="@../TealTeam.css" xmlns="http://javafx.com/javafx/10.0.2-internal" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.InputError">

    <center>
        <Label prefHeight="99.0" prefWidth="257.0" text="Error : Please enter valid inputs">
         <BorderPane.margin>
            <Insets top="15.0" />
         </BorderPane.margin></Label>
    </center>
   <bottom>
      <Button mnemonicParsing="false" onAction="#ClickedTryAgain" style="-fx-text-fill: black;" text="Try Again" BorderPane.alignment="CENTER">
         <BorderPane.margin>
            <Insets bottom="10.0" />
         </BorderPane.margin>
      </Button>
   </bottom>
   
</BorderPane>
